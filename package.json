  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.config.js",
    "build:styles": "webpack --config ./webpack.styles.config.js",
    "build:dev": "webpack --config ./webpack.config.js -w --progress",
    "dev": "npm run symlink && npm-run-all --parallel emulate build:dev",
    "start-and-dev": "npm-run-all --parallel dev start",
    "start-and-dev:css": "npm-run-all --parallel dev start:css",
    "start:css": "CSS_PATH=http://127.0.0.1:8080/dist/styles/antd.css webpack serve --config ./app/webpack.config.dev.js",
    "start": "webpack serve --config ./app/webpack.config.dev.js",
    "build:app": "webpack --config ./app/webpack.config.dev.js",
    "symlink": "ln -s -f ../../hooks/pre-push .git/hooks/pre-push",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
    "emulate": "divsrc-cli --emulate ./dist --port 5570",
    "bump": "npm version patch",
    "test:stage": "ENVIRONMENT=stage $(npm bin)/cypress run --headless -C cypress/config/staging.json --record --key 59b43619-5c9e-4b8b-998d-b8b35534fe94",
    "test:dev": "ENVIRONMENT=stage $(npm bin)/cypress run --headless -C cypress/config/staging.json",
    "cypress:dev": "ENVIRONMENT=development $(npm bin)/cypress open -C cypress/config/development.json",
    "cypress:stage": "ENVIRONMENT=stage $(npm bin)/cypress open -C cypress/config/staging.json",
    "cypress:prod": "ENVIRONMENT=production $(npm bin)/cypress open -C cypress/config/production.json",
    "divsrc:info": "divsrc-cli info --version $CODEBUILD_BUILD_NUMBER --artifact_id @supersonic-good-morning-mfe --secret $DIVSRC_SECRET_STAGE",
    "divsrc:push": "divsrc-cli push --version $CODEBUILD_BUILD_NUMBER --file index.js --artifact_id @supersonic-good-morning-mfe --url $BASE_URL --secret $DIVSRC_SECRET_STAGE",
    "divsrc:install": "divsrc-cli install --version $CODEBUILD_BUILD_NUMBER --file index.js --artifact_id @supersonic-good-morning-mfe --url $BASE_URL --secret $DIVSRC_SECRET_STAGE",
    "divsrc:install-prod": "divsrc-cli install --version $CODEBUILD_BUILD_NUMBER --file index.js --artifact_id @supersonic-good-morning-mfe --url $BASE_URL --secret $DIVSRC_SECRET_PROD"
  },
  "author": "ilya@supersonic.com",
